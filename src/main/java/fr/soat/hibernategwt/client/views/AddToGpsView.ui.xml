<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<c:CellTable ui:field="consultantsList" />


		<g:Grid>
			<g:row>
				<g:customCell>
					<g:Label text="Nom" width="33px" />
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="nom" />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Age" width="120px" />
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="age" />

				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Gps" width="120px" />
				</g:customCell>
				<g:customCell>
					<g:ListBox ui:field="gpsListBox" />
				</g:customCell>
			</g:row>
		</g:Grid>
		<g:Button text="ajouter" ui:field="addButton"></g:Button>
	</g:HTMLPanel>
</ui:UiBinder> 